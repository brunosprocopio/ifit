<p id="notice"><%= notice %></p>

<h1>Listing Dados Cartaos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Vencimento</th>
      <th>Numero</th>
      <th>Cod seguranca integer</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dados_cartaos.each do |dados_cartao| %>
      <tr>
        <td><%= dados_cartao.nome %></td>
        <td><%= dados_cartao.vencimento %></td>
        <td><%= dados_cartao.numero %></td>
        <td><%= dados_cartao.cod_seguranca_integer %></td>
        <td><%= dados_cartao.tipo %></td>
        <td><%= link_to 'Show', dados_cartao %></td>
        <td><%= link_to 'Edit', edit_dados_cartao_path(dados_cartao) %></td>
        <td><%= link_to 'Destroy', dados_cartao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dados cartao', new_dados_cartao_path %>
