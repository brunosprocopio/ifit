<%= form_for @usuario do |f| %>
    <p>
      <%= f.label :nome %> <br/>
      <%= f.text_field :nome %>
    </p>
    <p>
      <%= f.label :cpf %> <br/>
      <%= f.text_field :cpf %>
    </p>
    <p>
      <%= f.label :idade %> <br/>
      <%= f.text_field :idade %>
    </p>
